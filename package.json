  },
  "homepage": "https://github.com/koishijs/koishi-plugin-fortune",
  "dependencies": {
    "@stdlib/random-base-mt19937": "^0.0.5",
    "moment": "^2.29.1",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@koishijs/plugin-console": "^4.1.1",
    "@koishijs/plugin-database-memory": "^1.4.1",
    "@koishijs/plugin-sandbox": "^2.0.1",
