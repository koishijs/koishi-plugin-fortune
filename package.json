    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.4.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "ws": "^8.3.0"
  },
  "peerDependencies": {
    "koishi": "^4.8.2"
  },
  "jest": {
